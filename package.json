{
  "name": "mclauncher",
  "version": "1.0.0",
  "description": "Electron Launcher Minecraft",
  "main": "app.js",
  "scripts": {
    "start": ".\\node_modules\\electron\\dist\\electron.exe .",
    "build": "electron-build",
    "sass": "sass --watch ./sass/main.scss:./assets/css/style.css --style compressed",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/img/icon/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/img/icon/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Minecraft\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/img/icon/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Alex Pqn",
  "license": "ISC",
  "dependencies": {
    "browser-window": "^0.4.0",
    "custom-electron-titlebar": "^3.2.4",
    "electron": "^10.1.2",
    "fs": "0.0.1-security",
    "minecraft-launcher-core": "^3.15.0"
  },
  "devDependencies": {
    "electron-builder": "^22.8.0",
    "electron-packager": "^15.1.0",
    "electron-reload": "^1.5.0",
    "sass": "^1.26.10"
  }
}
